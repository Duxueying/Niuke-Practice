import java.util.*;
public class GetFrequency {
    public static int getFrequency(String[] article, int n, String word){
        int count=0;
        for(int i=0;i<article.length;i++){
            if(article[i].equals(word)){
              count++;
            }
        }
        return count;
    }

    public static void main(String[] args) {
        String[] article={"hello","world","hello","bit","hello"};
        System.out.println(getFrequency(article,5,"hello"));
    }
}
